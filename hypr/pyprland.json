{
  "pyprland": {
    "plugins": [
      "scratchpads",
      "magnify"
    ]
  },
  "scratchpads": {
    "term": {
      "command": "kitty --class kitty-pad",
      "animation": "fromTop",
      "margin": 50
    },
    "ranger": {
      "command": "kitty --class scratchpad -e ranger",
      "animation": "fromTop",
      "margin": 50
    },
    "volume": {
      "command": "pavucontrol",
      "animation": "fromRight"
    }
  }
}
